def Vec{x,y:Num}
vecs := [Vec{1,2}, Vec{3,4}, Vec{5,6}]
say "$:vecs $:vecs.x $:vecs.y"
fail unless "$vecs.x" == "[1, 3, 5]"
fail unless "$vecs.y" == "[2, 4, 6]"

def Boop{b:Bool,x:Int}
boops := [Boop{yes,123}, Boop{no,456}, Boop{yes,789}]
say "$:boops $:boops.b $:boops.x"
fail unless "$boops.b" == "[yes, no, yes]"
fail unless "$boops.x" == "[123, 456, 789]"

def Doop{x:Int,b:Bool}
doops := [Doop{123, yes}, Doop{456, no}, Doop{789, yes}]
say "$:doops $:doops.b $:doops.x"
fail unless "$doops.b" == "[yes, no, yes]"
fail unless "$doops.x" == "[123, 456, 789]"
