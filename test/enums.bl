enum Foo { A, B, C, D }

say "$(Foo.A)"
fail unless "$(Foo.A)" == "Foo.A"

for x in [Foo.A, Foo.B, Foo.C, Foo.D]
    when x is A
        say "$x is A!"
    is B
        say "$x is B!"
    else
        say "$x is something else"

enum Baz {
    Text{
        str:String
    }
    Integer{
        i:Int
    }
    Vec2{
        x,y:Num
    }
    Nil,
}

bazzes := [Baz.Text{"hi"}, Baz.Integer{5}, Baz.Vec2{2.4, 4.5}, Baz.Nil]

for b in bazzes
    when b is t:Text
        say "$(t.str)"
    is i:Integer
        say "$(i.i)"
    is v:Vec2
        say "$v"
    is Nil
        say "nil"

