
def doop(xs:[String])
    for i, x in xs
        say "$i) Loop: $x"
        prev := x
    between
        say "Between: $prev -> $x"
    say "Done!"

    for i,x in 5..+2..20
        say "$i) $x"
        prev := x
    between
        say "Between: $prev -> $x"


xs := ["one","two","three"]
doop(xs)
